{
  "name": "eoneopay/webhook",
  "description": "EoneoPay Webhook",
  "license": "MIT",
  "type": "package",
  "require": {
    "php": ">=7.1",
    "roave/security-advisories": "dev-master",
    "eoneopay/external": "dev-master",
    "illuminate/redis": "^5.5",
    "guzzlehttp/guzzle": "^6.0.0"
  },
  "require-dev": {
    "laravel/lumen-framework": "^5.5",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": { "EoneoPay\\Webhook\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Tests\\EoneoPay\\Webhook\\": "tests/" }
  },
  "provide": {
    "php-http/client-implementation": "1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "EoneoPay\\Webhook\\Bridge\\Laravel\\Providers\\WebhookServiceProvider",
        "EoneoPay\\Webhook\\Bridge\\Laravel\\Providers\\WebhookEventServiceProvider"
      ]
    }
  },
  "optimize-autoloader": true,
  "minimum-stability": "dev",
  "prefer-stable": true
}
